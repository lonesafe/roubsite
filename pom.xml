			<name>lonesafe</name>
			<email>lonesafe@qq.com</email>
			<url>https://www.roubsite.com</url>
		</developer>
	</developers>
	<modules>
		<module>RoubSiteSmarty4j</module>
		<module>RoubSiteFramework</module>
		<module>RoubSiteSecurity</module>
		<module>RoubSiteCodeSign</module>
		<module>RoubSiteUtils</module>
	</modules>
	<dependencies>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<version>4.0.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>druid</artifactId>
