	<packaging>pom</packaging>
	<version>4.1.3</version>
	<name>RoubSite</name>
	<description>RoubSite Framework parent project</description>
	<url>https://www.roubsite.com</url>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<roubsite_version>4.1.3</roubsite_version>
	</properties>
	<organization>
		<name>RoubSite</name>
		<url>https://www.roubsite.com</url>
	</organization>
	<scm>
		<connection>scm:git:git://github.com/lonesafe/roubsite</connection>
		<developerConnection>scm:git:git://github.com/lonesafe/roubsite</developerConnection>
		<url>https://github.com/lonesafe/roubsite</url>
